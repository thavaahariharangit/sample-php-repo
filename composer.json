{
  "name": "swatio/backend",
  "type": "project",
  "require": {
    "php": "^8.3",
    "ext-ctype": "*",
    "ext-filter": "*",
    "ext-hash": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-pcntl": "*",
    "ext-pdo": "*",
    "ext-posix": "*",
    "ext-redis": "^5.2",
    "ext-session": "*",
    "ext-zip": "*",
    "aws/aws-sdk-php": "^3.35",
    "bacon/bacon-qr-code": "^2.0",
    "caseyamcl/guzzle_retry_middleware": "^2.0",
    "egulias/email-validator": "^4",
    "facebook/graph-sdk": "^5.6",
    "fntneves/laravel-transactional-events": "^2.0",
    "google/apiclient": "^2.2",
    "guzzlehttp/guzzle": "^7",
    "guzzlehttp/promises": "^2.0.1",
    "guzzlehttp/psr7": "^2.1",
    "laravel/framework": "^10.0",
    "laravel/slack-notification-channel": "^3.0",
    "laravel/tinker": "^2.0",
    "lcobucci/jwt": "^4",
    "league/csv": "^9.7",
    "mfn/redlock-php": "0.0.0.1",
    "mll-lab/laravel-graphiql": "^3",
    "monolog/monolog": "^3",
    "nesbot/carbon": "^2.54",
    "nikic/php-parser": "^4.13",
    "nojimage/twitter-text-php": "^3.0",
    "opensearch-project/opensearch-php": "^2.1",
    "php-open-source-saver/jwt-auth": "^2",
    "phpoffice/phpspreadsheet": "^2.0",
    "pragmarx/google2fa-laravel": "^2",
    "psr/http-client": "^1.0",
    "psr/http-message": "^2.0",
    "psr/log": "^3.0",
    "psr/simple-cache": "^3.0",
    "ramsey/uuid": "^4",
    "rebing/graphql-laravel": "^9",
    "ruflin/elastica": "^7",
    "sentry/sentry": "^4",
    "sentry/sentry-laravel": "^4",
    "spatie/data-transfer-object": "^2",
    "spatie/laravel-csp": "2.*",
    "symfony/console": "^6",
    "symfony/http-client": "^7",
    "symfony/http-client-contracts": "^3.2",
    "symfony/http-foundation": "^6",
    "symfony/http-kernel": "^6",
    "symfony/intl": "^7",
    "symfony/mailer": "^6.2",
    "symfony/mime": "^6",
    "symfony/postmark-mailer": "^7",
    "symfony/process": "^6",
    "symfony/var-dumper": "^6",
    "thecodingmachine/safe": "^2",
    "webonyx/graphql-php": "^15",
    "wesbos/burner-email-providers": "dev-master"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^3",
    "brianium/paratest": "^7",
    "ergebnis/phpstan-rules": "^2",
    "fakerphp/faker": "^1.11",
    "infection/infection": "^0",
    "larastan/larastan": "^2",
    "mfn/php-cs-fixer-config": "^2",
    "mockery/mockery": "^1.0",
    "php-parallel-lint/php-parallel-lint": "^1.1",
    "phpstan/phpstan": "^1",
    "phpstan/phpstan-mockery": "^1.1",
    "phpstan/phpstan-phpunit": "^1.2",
    "phpstan/phpstan-strict-rules": "^1",
    "phpunit/php-code-coverage": "^10",
    "phpunit/phpunit": "^10",
    "rector/rector": "^1",
    "roave/security-advisories": "dev-master",
    "slam/phpstan-extensions": "^6.2",
    "spatie/phpunit-snapshot-assertions": "^5",
    "spaze/phpstan-disallowed-calls": "^3",
    "thecodingmachine/phpstan-safe-rule": "^1",
    "thecodingmachine/phpstan-strict-rules": "^1"
  },
  "autoload": {
    "files": [
      "app/helpers.php"
    ],
    "psr-4": {
      "Swat\\Api\\V2\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ],
    "psr-4": {
      "Swat\\Api\\V2\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "clear-caches": [
      "rm -f bootstrap/cache/*.php",
      "rm -f tmp/migration_hash.txt",
      "@php artisan optimize:clear"
    ],
    "ide-helper": [
      "@php artisan clear-compiled",
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:models --write --reset",
      "@php artisan ide-helper:meta",
      "@php artisan clear-compiled"
    ],
    "lint": "@php vendor/bin/parallel-lint --exclude vendor --exclude .phpstorm.meta.php --exclude _ide_helper.php --exclude .idea --blame .",
    "phpstan": "@php vendor/bin/phpstan analyse --configuration=phpstan.neon --error-format=raw app",
    "infection": [
      "@php -d enable_dl=On ./bin/infection_unittests.php",
      "mkdir -p tmp/infection/coverage/",
      "@putenv INFECTION=1",
      "@php -d extension=pcov ./vendor/bin/paratest --order-by=random --processes $(sysctl -n hw.ncpu || nproc || echo 4) --runner=WrapperRunner --configuration phpunit.infection.xml --coverage-xml=tmp/infection/coverage/coverage-xml --log-junit=tmp/infection/coverage/junit.xml",
      "@php vendor/bin/infection --min-msi=100 --min-covered-msi=100 --threads=$(sysctl -n hw.ncpu || nproc || echo 4) --skip-initial-tests --only-covered -s --coverage=tmp/infection/coverage"
    ],
    "infection-changed": [
      "@php -d enable_dl=On ./bin/infection_unittests.php",
      "mkdir -p tmp/infection/coverage/",
      "@putenv INFECTION=1",
      "@php -d extension=pcov ./vendor/bin/paratest --order-by=random --processes $(sysctl -n hw.ncpu || nproc || echo 4) --runner=WrapperRunner --configuration phpunit.infection.xml --coverage-xml=tmp/infection/coverage/coverage-xml --log-junit=tmp/infection/coverage/junit.xml",
      "@php vendor/bin/infection --git-diff-filter=AM --min-msi=100 --min-covered-msi=100 --threads=$(sysctl -n hw.ncpu || nproc || echo 4) --skip-initial-tests --only-covered -s --coverage=tmp/infection/coverage"
    ],
    "paratest": [
      "@php -d enable_dl=On ./bin/infection_unittests.php",
      "@putenv INFECTION=1",
      "@php ./vendor/bin/paratest --order-by=random --processes $(sysctl -n hw.ncpu || nproc || echo 4) --configuration phpunit.infection.xml"
    ],
    "phpstan-baseline": "@php vendor/bin/phpstan analyse --configuration=phpstan.neon app --generate-baseline",
    "php-cs-fixer": [
      "@php ./bin/php-cs-fixer.phar fix --diff"
    ],
    "phpstan-tests": [
      "@php vendor/bin/phpstan analyse --configuration=phpstan-tests.neon --error-format=raw tests"
    ],
    "phpstan-tests-baseline": [
      "@php vendor/bin/phpstan analyse --configuration=phpstan-tests.neon tests --generate-baseline=phpstan-tests-baseline.neon"
    ],
    "rector": [
      "@rector-app",
      "@rector-tests"
    ],
    "rector-app": [
      "@php vendor/bin/rector process --config=rector-app.php"
    ],
    "rector-tests": [
      "@php vendor/bin/rector process --config=rector-tests.php"
    ],
    "detect-short-open-tag": "detect_short_open_tag .",
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "config": {
    "optimize-autoloader": false,
    "preferred-install": "dist",
    "autoloader-suffix": "SwatioApiV2",
    "sort-packages": true,
    "allow-plugins": {
      "infection/extension-installer": true,
      "composer/package-versions-deprecated": true,
      "php-http/discovery": false
    },
    "process-timeout": 0
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "facebook/graph-sdk",
        "type": "library",
        "version": "5.7.0",
        "require": {
          "php": "^5.4|^7.0|^8.0"
        },
        "dist": {
          "url": "https://github.com/facebookarchive/php-graph-sdk/archive/refs/tags/5.7.0.zip",
          "type": "zip"
        },
        "autoload": {
          "psr-4": {
            "Facebook\\": "src/Facebook/"
          },
          "files": ["src/Facebook/polyfills.php"]
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "mfn/redlock-php",
        "type": "library",
        "version": "0.0.0.1",
        "dist": {
          "url": "https://codeload.github.com/mfn/redlock-php/legacy.zip/8f45575379c4e6b728771d19a59c224e141705c2",
          "type": "zip"
        },
        "autoload": {
          "classmap": [
            "src/"
          ]
        }
      }
    }
  ],
  "extra": {
    "laravel": {
      "dont-discover": [
        "php-open-source-saver/jwt-auth"
      ]
    }
  }
}
